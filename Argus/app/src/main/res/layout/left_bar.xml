<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/left"
    android:layout_width="90dp"  android:layout_height="180dp"
    android:background="@drawable/left" >
    <com.example.user.argus.floatDragon_ui.MoveButton
        android:id="@+id/left_right"
        android:layout_width="20dip"   android:layout_height="20dip"
        android:layout_centerVertical="true"   android:layout_margin="10dip"
        android:layout_alignParentRight="true"/>
    <com.example.user.argus.floatDragon_ui.SettingButton
        android:id="@+id/left_top"
        android:layout_width="20dip"    android:layout_height="20dp"
        android:layout_alignParentTop="true"  android:layout_margin="10dip"/>
    <com.example.user.argus.floatDragon_ui.AppButton
        android:id="@+id/left_bottom"
        android:layout_width="20dip"     android:layout_height="20dip"
        android:layout_alignParentBottom="true"
        android:layout_margin="10dip"/>


    <com.example.user.argus.floatDragon_ui.LockButton
        android:id="@+id/left_rt"
        android:layout_width="20dip"    android:layout_height="20dp"
        android:layout_alignParentRight="true"  android:layout_alignParentTop="true"

        android:layout_marginTop="44dp"        android:layout_marginRight="35dp" />

    <com.example.user.argus.floatDragon_ui.NoteButton
        android:id="@+id/left_rb"
        android:layout_width="20dip"    android:layout_height="20dp"
        android:layout_alignParentRight="true"  android:layout_alignParentBottom="true"
        android:layout_marginBottom="44dp"        android:layout_marginRight="35dp" />


</RelativeLayout>
